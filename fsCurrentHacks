-- Minimize state
local isMinimized = false
local function toggleMinimize()
    isMinimized = not isMinimized
    if isMinimized then
        frame.Size = UDim2.new(0, 300, 0, 90)
        outerFrame.Size = UDim2.new(0, 310, 0, 100)
        purchaseButton.Visible = false
        equipButton.Visible = false
        moneyButton.Visible = false
        minimizeButton.Text = "+"
    else
        frame.Size = UDim2.new(0, 300, 0, 340)
        outerFrame.Size = UDim2.new(0, 310, 0, 350)
        purchaseButton.Visible = true
        equipButton.Visible = true
        moneyButton.Visible = true
        minimizeButton.Text = "-"
    end
end

minimizeButton.MouseButton1Click:Connect(toggleMinimize)

-- Connect Purchase Button
purchaseButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
    loadstring(purchaseGlitcherScript)()
end)

-- Connect Equip Button
equipButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
    loadstring(equipGlitcherScript)()
end)

-- Connect Money Button
